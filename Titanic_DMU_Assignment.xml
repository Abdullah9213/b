<_ROOT_ EMVERSION="15.3" ORIENTATION="HORIZONTAL">
  <Workspace>
    <NODES>

      <!-- 1. File Import -->
      <NODE CLASS="SASHELP.EMCORE.EXTENSION.CLASS"
            COMPONENT="File Import"
            ID="FIMPORT"
            X="100" Y="300"
            DESCRIPTION="Import Titanic CSV">
        <PROPERTIES
          FileName="H:\Downloads\titanic.csv"
          FileType="csv"
          Delimiter=","
          GuessRows="500"
          NameRow="Y"
          Role="TRAIN"
          Seed="42539"
          AdvancedAdvisor="Y" />
      </NODE>

      <!-- 2. Data Partition -->
      <NODE CLASS="SASHELP.EMCORE.EXTENSION.CLASS"
            COMPONENT="Data Partition"
            ID="DPART"
            X="350" Y="300"
            DESCRIPTION="Partition Data 60/40">
        <PROPERTIES
          TrainPercent="60"
          ValidatePercent="40"
          TestPercent="0"
          Seed="42539"
          AdvancedAdvisor="Y" />
      </NODE>

      <!-- 3. Decision Tree -->
      <NODE CLASS="SASHELP.EMCORE.EXTENSION.CLASS"
            COMPONENT="Tree"
            ID="TREE"
            X="600" Y="200"
            DESCRIPTION="Decision Tree Model">
        <PROPERTIES
          Criterion="Gini"
          MaxDepth="10"
          AdvancedAdvisor="Y" />
      </NODE>

      <!-- 4. Regression -->
      <NODE CLASS="SASHELP.EMCORE.EXTENSION.CLASS"
            COMPONENT="Regression"
            ID="REG"
            X="600" Y="400"
            DESCRIPTION="Logistic Regression">
        <PROPERTIES
          ModelType="Logistic"
          SelectionMethod="Stepwise"
          AdvancedAdvisor="Y" />
      </NODE>

      <!-- 5. Model Comparison -->
      <NODE CLASS="SASHELP.EMCORE.EXTENSION.CLASS"
            COMPONENT="Model Comparison"
            ID="MCMP"
            X="850" Y="300"
            DESCRIPTION="Compare Models">
        <PROPERTIES
          SelectionStatistic="ASE"
          AdvancedAdvisor="Y" />
      </NODE>

    </NODES>

    <!-- Node connections -->
    <CONNECTIONS>
      <CONNECTION FROM="FIMPORT" TO="DPART" />
      <CONNECTION FROM="DPART" TO="TREE" />
      <CONNECTION FROM="DPART" TO="REG" />
      <CONNECTION FROM="TREE" TO="MCMP" />
      <CONNECTION FROM="REG" TO="MCMP" />
    </CONNECTIONS>

    <NOTES>
      <DATA>Working Titanic flow - File Import → Partition → Tree + Regression → Model Comparison</DATA>
    </NOTES>

  </Workspace>
</_ROOT_>
